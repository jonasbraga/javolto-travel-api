{
  "name": "infrastructure",
  "version": "1.0.0",
  "license": "MIT-0",
  "author": "Jack Stevenson",
  "private": true,
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile && ./bin/build-chrome-layer.sh",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:destroy": "cdk --profile jbAccount destroy '*' --require-approval never",
    "precdk:deploy": "npm ci && npm run build",
    "cdk:deploy": "cdk --profile jbAccount deploy '*' --require-approval never"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.3.2",
    "@types/lodash": "^4.14.182",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "aws-cdk": "2.29.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "puppeteer": "10.1.0",
    "node-fetch": "^3.2.6",
    "esbuild": "0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.29.1",
    "constructs": "10.1.42",
    "source-map-support": "^0.5.21",
    "puppeteer-core": "10.1.0",
    "chrome-aws-lambda": "10.1.0",
    "commander": "^9.3.0",
    "aws-sdk": "^2.1163.0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  },
  "overrides": {
    "node-fetch": "$node-fetch"
  }
}
